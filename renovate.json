{
  "extends": [
    "config:base"
  ],
  "enabledManagers": [
    "regex",
    "github-actions"
  ],
  "labels": ["r:{{depName}}", "r:{{depName}}/{{newVersion}}"],
  "regexManagers": [
    {
      "fileMatch": ["^import_map.json$"],
      "matchStrings": [
        "https:\/\/deno.land\/x\/(?<depName>[^@]+)@(?<currentValue>v?[0-9.]+)"
      ],
      "currentValueTemplate": "{{currentValue}}",
      "datasourceTemplate": "npm"
    }
  ]
}
